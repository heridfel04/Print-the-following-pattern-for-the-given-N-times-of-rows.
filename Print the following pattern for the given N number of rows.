import java.util.*;

public class Rectangle {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number:");
        int n = sc.nextInt();
        
        int k=0;
        while(k<=n)
        {
            System.out.println();

            for(int i=1;i<k;i++)
            {
                System.out.print('*');
            }
            k++;
        }
    }
}
